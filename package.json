    "typescript": "~4.5.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.52.0",
    "@aws-sdk/client-lambda": "^3.52.0",
    "@aws-sdk/client-s3": "^3.52.0",
    "@aws-sdk/lib-dynamodb": "^3.52.0",
    "aws-cdk-lib": "2.10.0",
    "aws-sdk": "^2.1077.0",
    "constructs": "^10.0.67",
    "csvtojson": "^2.0.10"
  }
